<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane id="AnchorPane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="schedulekeeper.view.MenuController">
   <children>
      <HBox minHeight="400.0" minWidth="600.0">
         <children>
            <VBox alignment="CENTER" minHeight="600.0" minWidth="900.0" spacing="50.0">
               <children>
                  <Label fx:id="welcomeLabel" alignment="CENTER" contentDisplay="CENTER" minHeight="30.0" minWidth="250.0" text="Label" />
                  <Label fx:id="dateLabel" alignment="CENTER" contentDisplay="CENTER" minHeight="30.0" minWidth="250.0" text="Label" />
               </children>
            </VBox>
            <VBox alignment="CENTER" minHeight="600.0" minWidth="300.0" spacing="25.0">
               <children>
                  <Button minWidth="150.0" mnemonicParsing="false" onAction="#appointmentsClick" text="Appointments" />
                  <Button minWidth="150.0" mnemonicParsing="false" onAction="#customersClick" text="Customers" />
                  <Button minWidth="150.0" mnemonicParsing="false" onAction="#reportsClick" text="Reports" />
                  <Button minWidth="150.0" mnemonicParsing="false" onAction="#userlogClick" text="User Log" />
                  <Button minWidth="150.0" mnemonicParsing="false" onAction="#logoutClick" text="Logout" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
