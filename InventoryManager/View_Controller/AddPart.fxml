<Pane fx:id="addPartScene" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="750.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.AddPartController">
   <children>
      <VBox alignment="CENTER" prefHeight="750.0" prefWidth="900.0">
         <children>
            <HBox alignment="CENTER">
               <children>
                  <Label text="Add Part">
                     <font>
                        <Font name="Consolas Bold" size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="20.0" right="20.0" />
                     </HBox.margin>
                  </Label>
                  <RadioButton mnemonicParsing="false" text="In-House">
                     <HBox.margin>
                        <Insets left="20.0" right="20.0" />
                     </HBox.margin>
                  </RadioButton>
                  <RadioButton mnemonicParsing="false" text="Outsourced">
                     <HBox.margin>
                        <Insets left="20.0" right="20.0" />
                     </HBox.margin>
                  </RadioButton>
               </children>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER">
               <children>
                  <VBox alignment="CENTER_LEFT" spacing="20.0">
                     <children>
                        <Label prefHeight="25.0" text="ID">
                           <font>
                              <Font name="Consolas Bold" size="14.0" />
                           </font>
                        </Label>
                        <Label prefHeight="25.0" text="Name">
                           <font>
                              <Font name="Consolas Bold" size="14.0" />
                           </font>
                        </Label>
                        <Label prefHeight="25.0" text="Inv">
                           <font>
                              <Font name="Consolas Bold" size="14.0" />
                           </font>
                        </Label>
                        <Label prefHeight="25.0" text="Price/Cost">
                           <font>
                              <Font name="Consolas Bold" size="14.0" />
                           </font>
                        </Label>
                        <Label prefHeight="25.0" text="Max">
                           <font>
                              <Font name="Consolas Bold" size="14.0" />
                           </font>
                        </Label>
                        <Label text="Min">
                           <font>
                              <Font name="Consolas Bold" size="14.0" />
                           </font>
                        </Label>
                        <Label prefHeight="25.0" text="Machine ID">
                           <font>
                              <Font name="Consolas Bold" size="14.0" />
                           </font>
                        </Label>
                     </children>
                     <HBox.margin>
                        <Insets right="30.0" />
                     </HBox.margin>
                  </VBox>
                  <VBox alignment="CENTER_LEFT" spacing="20.0">
                     <children>
                        <TextField prefWidth="100.0" />
                        <TextField prefWidth="100.0" />
                        <TextField prefWidth="100.0" />
                        <TextField prefWidth="100.0" />
                        <TextField prefWidth="100.0" />
                        <TextField prefWidth="100.0" />
                        <TextField prefWidth="100.0" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="CENTER" spacing="30.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#savePartButtonClick" prefWidth="60.0" text="Save" />
                  <Button mnemonicParsing="false" onAction="#cancelButtonClick" prefWidth="60.0" text="Cancel" />
               </children>
               <padding>
                  <Insets bottom="20.0" top="20.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</Pane>
